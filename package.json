    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@types/bun": "^1.2.19",
    "@types/node": "^22.17.0",
    "daisyui": "^5.0.50",
    "drizzle-kit": "^0.30.6",
    "eslint": "^9.32.0",
